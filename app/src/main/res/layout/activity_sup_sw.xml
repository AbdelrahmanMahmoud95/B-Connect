<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView   xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/sv_sup_sw"
    >


    <LinearLayout
        android:id="@+id/ll_sup_sw"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        android:orientation="vertical"
        android:padding="15dp"

        android:paddingTop="40dp">

        <Button
            android:id="@+id/but_supm"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="20dp"
            android:background="@color/design_default_color_primary"
            android:drawableRight="@android:drawable/btn_star_big_on"
            android:text="@string/title_activity_sup_m"
            android:textColor="#ffff"
            android:textSize="25dp" />
        <Button
            android:id="@+id/but_supo"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginBottom="20dp"
            android:background="@color/design_default_color_primary"
            android:drawableRight="@android:drawable/btn_star_big_on"
            android:text="@string/title_activity_sup_o"
            android:textColor="#ffff"
            android:textSize="25dp" />



    </LinearLayout>
</android.support.v4.widget.NestedScrollView>